def find_between_YOUR_ATTRIBUTE(record):
    for idxs in from_until("FROM_WORD", "TO_WORD", record):
        yield "your-label", idxs[0] + 1, idxs[1] - 1


def from_until(from_token, until_token, record):
    collect_idxs = []
    start_idx, end_idx = None, None
    current_candidate = False
    for token in record["BODY"]:
        if token.text == from_token:
            current_candidate = True
            start_idx = token.i + 1
        if token.text == until_token and current_candidate:
            end_idx = token.i - 1
            collect_idxs.append([start_idx, end_idx])
            current_candidate = False
    return collect_idxs
